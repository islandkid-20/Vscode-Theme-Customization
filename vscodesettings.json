{
  "editor.fontFamily": "Dank Mono",
  // "editor.fontFamily": "Fragment Mono",
  // "editor.fontFamily": "Roboto",
  // "editor.fontWeight": "bold",
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": ["comment", "keyword", "variable.language"],
        "settings": {
          "fontStyle": "italic"
        }
      }
    ]
  },
  "editor.fontLigatures": true,
  "editor.formatOnSave": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[javascript, javascriptreact]" : {
    "editor.formatOnSave": true
  },
  "breadcrumbs.enabled": false,
  "editor.minimap.enabled": false,
  "editor.fontSize": 12,
  "editor.lineNumbers": "on",
  "explorer.confirmDragAndDrop": false,
  "editor.wordWrap": "on",
  "explorer.confirmDelete": false,
  "editor.cursorBlinking": "expand",
  "editor.find.cursorMoveOnType": false,
  "editor.cursorSmoothCaretAnimation": "on",
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "terminal.integrated.enableMultiLinePasteWarning": "never",
  "git.openRepositoryInParentFolders": "never",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "editor.accessibilitySupport": "off",
  "terminal.integrated.env.windows": {},
  "git.autofetch": true,
  "workbench.statusBar.visible": true,
  "workbench.colorTheme": "Bearded Theme Arc Blueberry",
  "window.menuBarVisibility": "visible",
  "workbench.layoutControl.enabled": false,
  "window.commandCenter": false,
  "window.zoomLevel": 1.5,
  "workbench.colorCustomizations": {},
  "workbench.startupEditor": "none",

  "terminal.integrated.fontSize": 11,
  "terminal.integrated.fontFamily": "Dank Mono",
  "workbench.iconTheme": "moxer-icons",

  "files.associations": {
    "*.html": "html",
    "*.css": "tailwindcss"
  },
  "codeium.enableConfig": {
    "*": true,
    "properties": true
  },
  "hediet.vscode-drawio.resizeImages": null,
  "terminal.integrated.env.linux": {},
  "console-ninja.featureSet": "Community",
  "liveServer.settings.donotShowInfoMsg": true,
  "console-ninja.warnOnNoToolsDetected": false,
  "window.customTitleBarVisibility": "auto",
  "workbench.activityBar.location": "hidden"
}
